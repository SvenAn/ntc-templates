Value PORT (\d+)
Value NAME (\S*)
Value VLAN (\S*)
Value STATE ([DEFL])
Value LINKSTATE ([ARLDdB|NP])
Value SPEED (\d*)
Value DUPLEX ([HALF|FULL]*)

Start
  ^Port.*
  ^.*String.*VLANs.*State\sState\sActual\sActual
  ^\=+
  ^${PORT}\s+${NAME}\s+${VLAN}\s+${STATE}\s+${LINKSTATE}\s+${SPEED}\s+${DUPLEX} -> Record
  ^\s+Port\sState: -> Done
  ^\s+-+\s+-+\s+(\+\s+|)-+\s+-+(\s+-+|)\s*$$
  ^\s*$$
  ^. -> Error

Done
  ^.*
